{$R *.dfm}

procedure TDM.adodsWorkCurrDiscCalcFields(DataSet: TDataSet);
begin
  DM.adodsWorkCurrDiscSumClassHour.Value :=
      DM.adodsWorkCurrDiscHourLecture.AsInteger+
      DM.adodsWorkCurrDiscHourSeminar.AsInteger+
      DM.adodsWorkCurrDiscHourPractice.AsInteger+
      DM.adodsWorkCurrDiscHourSmallGroup.AsInteger+
      DM.adodsWorkCurrDiscHourIndividual.AsInteger;

  DM.adodsWorkCurrDiscSumHour.Value :=
      DM.adodsWorkCurrDiscHourLecture.AsInteger+
      DM.adodsWorkCurrDiscHourSeminar.AsInteger+
      DM.adodsWorkCurrDiscHourPractice.AsInteger+
      DM.adodsWorkCurrDiscHourSmallGroup.AsInteger+
      DM.adodsWorkCurrDiscHourIndividual.AsInteger+
      DM.adodsWorkCurrDiscHourSelfWork.AsInteger;

  if(DM.adodsWorkCurriculumPlanGeneration.AsInteger=3)and(DM.adodsWorkCurrDiscControlExamination.AsBoolean)
  then DM.adodsWorkCurrDiscSumHour.Value := DM.adodsWorkCurrDiscSumHour.Value + 36;
end;

