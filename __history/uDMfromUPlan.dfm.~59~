      'atsii] sz on wp.specializ=sz.id'#13#10#9'join [Kvalifikatsii] kv on wp.' +
      'kvalif=kv.id'#13#10#9'join [Sokr] as sk on wp.sokr_id=sk.id'#13#10#9'where wp.' +
      'id=:idPlan'
